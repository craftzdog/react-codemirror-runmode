{
  "name": "react-codemirror-runmode",
  "version": "1.0.4",
  "description": "Syntax highlighting for react, utilizing CodeMirror's parser",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src -d lib",
    "watch": "babel src --watch -d lib",
    "watch:demo": "webpack --progress --watch --config webpack.config.demo.babel.js",
    "lint": "eslint src --ext .js --ext .jsx",
    "test": "mocha --require @babel/register --require test/setup.js",
    "prepublishOnly": "npm run build && npm run lint && npm test"
  },
  "keywords": [
    "remark",
    "react",
    "codemirror",
    "syntax",
    "highlight"
  ],
  "author": "Takuya Matsuyama <hi@craftz.dog>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/craftzdog/react-codemirror-runmode.git"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.5.5",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-add-module-exports": "^0.2.1",
    "codemirror": "^5.48.2",
    "core-js": "^3.1.4",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "jsdom": "^15.1.1",
    "mocha": "^6.2.0",
    "prettier": "^1.18.2",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-test-renderer": "^16.8.6",
    "remark": "^11.0.1",
    "remark-react": "^6.0.0",
    "webpack": "^4.37.0",
    "webpack-cli": "^3.3.6"
  }
}
