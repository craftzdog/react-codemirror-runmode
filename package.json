{
  "name": "react-codemirror-runmode",
  "version": "1.0.0",
  "description": "Syntax highlighting for remark-react through CodeMirror",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src -d lib",
    "watch": "babel src --watch -d lib",
    "watch:demo": "webpack --progress --watch --config webpack.config.demo.babel.js",
    "lint": "eslint src --ext .js --ext .jsx",
    "test": "npm run lint && mocha --require test/setup.js --compilers js:babel-register"
  },
  "keywords": [
    "remark",
    "react",
    "codemirror",
    "syntax",
    "highlight"
  ],
  "author": "Takuya Matsuyama <hi@craftz.dog>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-0-without-async": "^1.0.0",
    "babel-register": "^6.26.0",
    "codemirror": "^5.30.0",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "eslint": "^4.8.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "jsdom": "^11.3.0",
    "mocha": "^4.0.1",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-test-renderer": "^16.0.0",
    "remark": "^8.0.0",
    "remark-react": "^4.0.1",
    "webpack": "^3.6.0"
  }
}
